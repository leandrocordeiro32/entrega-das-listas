package br.com.voeairlines.treinamentopoo;

import java.util.Scanner;

public class Principal {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner sc = new Scanner (System.in);
		
		System.out.print("Escolha uma opção de 1 a 26: ");
		
		int opcao = Integer.parseInt(sc.nextLine());
		
		switch(opcao) {
		
		case 1:
			
			/*Algoritmo01 alg1 = new Algoritmo01();
			System.out.print("Resultado: " + alg1.imprimirMensagem());*/
			
			System.out.println("Resultado = " + new Algoritmo01().imprimirMensagem());
			
			break;
			
		case 2:
			
			new Algoritmo02().imprimaNome();
			
			break;
			
		case 3:
			
			System.out.println(new Algoritmo03().imprimeProduto());
			
			break;
			
		case 4:
			
			System.out.println("A média de 8 , 9 e 7 é igual a " + new Algoritmo04().imprimeMedia());
			
			break;
			
		case 5:
			
			Algoritmo05 alg5 = new Algoritmo05();
			System.out.print("Informe um número inteiro: ");
			alg5.setNumero(sc.nextInt());
			
			System.out.println(alg5);
			
			break;
			
		case 6:
			Algoritmo06 alg6 = new Algoritmo06();
			System.out.print("Informe um número inteiro: ");
			alg6.setNumero1(sc.nextInt());
			System.out.print("Informe outro número inteiro: ");
			alg6.setNumero2(sc.nextInt());
			
			System.out.println(alg6);
			
		}
		
		sc.close();
	}

}
