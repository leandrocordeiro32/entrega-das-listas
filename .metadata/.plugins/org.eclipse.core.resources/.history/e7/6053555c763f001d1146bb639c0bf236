package br.com.voeairlines.treinamentopoo;

import java.util.Scanner;

public class Principal {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner sc = new Scanner (System.in);
		
		System.out.print("Escolha uma opção de 1 a 26: ");
		
		int opcao = Integer.parseInt(sc.nextLine());
		
		switch(opcao) {
		
		case 1:
			
			/*Algoritmo01 alg1 = new Algoritmo01();
			System.out.print("Resultado: " + alg1.imprimirMensagem());*/
			
			System.out.println("Resultado = " + new Algoritmo01().imprimirMensagem());
			
			break;
			
		case 2:
			
			System.out.println("Resultado = " + new Algoritmo02().imprimaNome());
			
			break;
			
		case 3:
			
			System.out.println(new Algoritmo03().imprimeProduto());
			
			break;
			
		case 4:
			
			System.out.println("A média de 8 , 9 e 7 é igual a " + new Algoritmo04().imprimeMedia());
			
			break;
			
		case 5:
			System.out.print("Informe um número inteiro: ");
			new Algoritmo05().setNumero(sc.nextInt());
			new Algoritmo05();
		}
		
		sc.close();
	}

}
